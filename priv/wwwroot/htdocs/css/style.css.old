* {
	font-family: Calibri, Arial, sans-serif;
	text-align: center;
	margin: 0;
}

div#wrapper {
	width: 900px;
	margin: 0 auto;
}

H1 {
	text-align: left;
	width: 100%;
	background-color: #336699;
	color: white;
	font-size: 16px;
	font-weight: bold;
}

TH {
	text-align: center;
	font-size: 14px;
	background-color: #f0f0ff;
}

TD {
	text-align: center;
	font-size: 12px;
}

.fileName {
	text-align: left;
}


.rowOdd {
	background-color: #f8f8f8;
}

.doneRow {
	padding: 5px;
}

.availableRow {
	padding: 5px;
}


